  "components": {
    "schemas": {
      "UserResponseDto": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "surname": {
            "type": "string"
          },
          "dateOfBirth": {
            "format": "date-time",
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "isAdmin": {
            "type": "boolean"
          },
          "createdAt": {
            "format": "date-time",
            "type": "string"
          },
          "updatedAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "_id",
          "name",
          "surname",
          "dateOfBirth",
          "email",
          "isAdmin",
          "createdAt",
          "updatedAt"
        ]
      },
      "PaginationMetaDto": {
        "type": "object",
        "properties": {
          "perPage": {
            "type": "number",
            "example": 10
          },
          "totalPages": {
            "type": "number",
            "example": 1
          },
          "currentPage": {
            "type": "number",
            "example": 1
          },
          "pageStartIndex": {
            "type": "number",
            "example": 1
          },
          "hasPrev": {
            "type": "boolean",
            "example": false
          },
          "hasNext": {
            "type": "boolean",
            "example": false
          },
          "prev": {
            "type": "object",
            "example": null,
            "nullable": true
          },
          "next": {
            "type": "object",
            "example": null,
            "nullable": true
          }
        },
        "required": [
          "perPage",
          "totalPages",
          "currentPage",
          "pageStartIndex",
          "hasPrev",
          "hasNext",
          "prev",
          "next"
        ]
      },
      "UsersResponseDataDto": {
        "type": "object",
        "properties": {
          "users": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/UserResponseDto"
            }
          },
          "pagination": {
            "$ref": "#/components/schemas/PaginationMetaDto"
          },
          "totalUsers": {
            "type": "number",
            "example": 100
          }
        },
        "required": [
          "users",
          "pagination",
          "totalUsers"
        ]
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "User name",
            "example": "John"
          },
          "surname": {
            "type": "string",
            "description": "User surname",
            "example": "Doe"
          },
          "dateOfBirth": {
            "format": "date-time",
            "type": "string",
            "description": "User date of birth",
            "example": "11.09.1990"
          },
          "email": {
            "type": "string",
            "description": "User email",
            "example": "johndoe@mail.com"
          },
          "password": {
            "type": "string",
            "description": "User password",
            "example": "P@ssw0rd123",
            "minLength": 8,
            "maxLength": 64
          },
          "oldPassword": {
            "type": "string",
            "description": "User old password",
            "example": "P@ssw0rd123"
          }
        }
      },
      "CreateUserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "User name",
            "example": "John"
          },
          "surname": {
            "type": "string",
            "description": "User surname",
            "example": "Doe"
          },
          "dateOfBirth": {
            "type": "string",
            "description": "User date of birth",
            "example": "1990-09-11"
          },
          "email": {
            "type": "string",
            "description": "User email",
            "example": "johndoe@mail.com"
          },
          "password": {
            "type": "string",
            "description": "User password",
            "example": "P@ssw0rd123",
            "minLength": 8,
            "maxLength": 64
          }
        },
        "required": [
          "name",
          "surname",
          "dateOfBirth",
          "email",
          "password"
        ]
      },
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User email",
            "example": "johndoe@mail.com"
          },
          "password": {
            "type": "string",
            "description": "User password",
            "example": "P@ssw0rd123"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "LogoutResponseDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "User has been signed out"
          }
        },
        "required": [
          "message"
        ]
      },
      "CreatePostDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Post title"
          },
          "content": {
            "type": "string",
            "description": "Post content"
          },
          "image": {
            "type": "string",
            "description": "Post image url",
            "example": "https://firebasestorage.googleapis.com/exampleImage.jpg"
          }
        },
        "required": [
          "title",
          "content",
          "image"
        ]
      },
      "UpdatePostDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Post title"
          },
          "content": {
            "type": "string",
            "description": "Post content"
          },
          "image": {
            "type": "string",
            "description": "Post image url",
            "example": "https://firebasestorage.googleapis.com/exampleImage.jpg"
          }
        }
      }
    }
